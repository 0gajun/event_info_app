<% provide(:title, '場所新規登録') %>
<div class="row">
	<div class="col-md-6 col-md-offset-3">
		<h2>場所新規登録</h2>
		<%= form_for(@place) do |f| %>
			<%= f.label :name , "名前", class: 'control-label' %>
			<%= f.text_field :name ,class: 'form-control'%>

			<%= f.submit "変更を保存", class: "btn btn-large btn-primary" %>
		<% end %>
	</div>
</div>